{  "name": "uwdoem/rest-backend-propel-respondor",
    "description": "Respondor for the UWDOEM REST backend framework",
    "license": "MIT",
    "require": {
        "slim/slim": "^3.8",
        "propel/propel": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "fzaninotto/faker": "^1.7",
        "jaschilz/php-coverage-badger": "2.*"
    },
    "autoload": {
        "classmap": [
            "schema/generated-classes/",
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "test/"
        ]
    }
}
